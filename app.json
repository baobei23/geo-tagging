{
  "expo": {
    "name": "geo-tagging",
    "slug": "geo-tagging",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "geotagging",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Aplikasi ini memerlukan akses kamera untuk mengambil foto usaha."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Aplikasi ini memerlukan akses lokasi untuk menandai geo-tag pada foto usaha."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Aplikasi ini memerlukan akses galeri untuk menyimpan foto yang diambil.",
          "savePhotosPermission": "Aplikasi ini memerlukan izin untuk menyimpan foto ke galeri."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
